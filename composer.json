{
  "name": "minube/geo-timezone",
  "homepage": "http://minube.com",
  "description": "PHP library for timezone calculation from a particular location and date.",
  "license": "MIT",
  "require": {
    "php": "^8.1.0",
    "ext-dom": "*",
    "ext-mbstring": "^8.1.0",
    "ext-zip": "*",
    "ext-xml":"^8.1.0",
    "phayes/geophp": "1.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.0",
    "guzzlehttp/guzzle": "^7.4.0"
  },
  "autoload": {
    "psr-4": {
      "GeoTimeZone\\": "src/GeoTimeZone/",
      "Tests\\": "tests/"
    }
  },
  "config": {
    "vendor-dir": "vendor"
  }
}
